{
  "name": "avktechno-frontend",
  "version": "1.0.0",
  "description": "Avktechno frontend side",
  "author": "avmax <avmax.web@gmail.com>",
  "private": true,
  "config": {
    "nuxt": {
      "port": 4252
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV='development' PORT=4252 nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "prod:start": "cross-env NODE_ENV='production' PORT=4252 nuxt build && nuxt start",
    "prod:generate": "cross-env NODE_ENV='production' PORT=4252 nuxt build && nuxt generate"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "cross-env": "^5.1.3",
    "lodash": "^4.17.4",
    "nuxt": "^1.0.0-rc11",
    "vue-spinner": "^1.0.3",
    "vue2-google-maps": "^0.8.4",
    "vuetify": "^0.17.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.7.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.8",
    "postcss-nested": "^2.1.2",
    "postcss-responsive-type": "^1.0.0",
    "sass-loader": "^6.0.6",
    "scss-loader": "^0.0.1"
  }
